nota1 = float(input('digite a primeira nota :'))
nota2 = float(input('digite a segunda nota :'))
nota3 = float(input('digite a terceira nota :'))

nota4 = nota1+nota2+nota3 
media = nota4 / 3

print ('sua media foi ',media)

if media>6:
    print ('aprovado')
elif 4<=media>=6:
    print('Prova final')
elif media<4 :
    print('reprovado')
